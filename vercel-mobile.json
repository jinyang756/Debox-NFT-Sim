{
  "version": 2,
  "builds": [
    {
      "src": "mobile-server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "public/mobile/**",
          "public/js/mobile-storage.js",
          "public/js/env-config.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mobile-server.js"
    }
  ],
  "github": {
    "enabled": false
  },
  "public": true
}